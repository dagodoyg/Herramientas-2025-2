   ],
   "source": [
    "N=1000000\n",
    "Nc=0\n",
    "\n",
    "for ii in range(N):\n",
    "    x = np.random.rand()\n",
    "    y = np.random.rand()\n",
    "    if x**2+y**2<=1:\n",
    "        Nc += 1\n",
    "print(\"Montecarlo pi: \", 4*Nc/N)"
   ]
